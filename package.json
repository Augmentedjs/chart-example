{
  "name": "chart-example",
  "version": "1.0.0",
  "description": "A simple class that renders a table like a bar chart",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --module-bind 'css=style-loader!css-loader' --progress",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node ./service/index.js",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/augmentedjs/chart-example.git"
  },
  "author": "Bob Warren <robert_warren@gap.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/augmentedjs/chart-example/issues"
  },
  "homepage": "https://github.com/augmentedjs/chart-example#readme",
  "dependencies": {
    "compression": "^1.7.3",
    "presentation-chart": "^1.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
